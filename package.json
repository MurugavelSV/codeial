  "description": "A web app to interact with everyone similar to a social media web page",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index",
    "home-scss": "sass --no-source-map --watch assets/scss/home.scss assets/css/home.css",
    "layout-scss": "sass --no-source-map --watch assets/scss/layout.scss assets/css/layout.css",
    "dev": "npm-run-all --parallel home-scss layout-scss start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "social"
  ],
