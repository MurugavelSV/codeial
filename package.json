  "license": "ISC",
  "dependencies": {
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "mongoose": "^7.4.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "sass": "^1.66.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
